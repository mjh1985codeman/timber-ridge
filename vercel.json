{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/client",
            "headers": {
                "cache-control": "s-maxage=31536000,immutable"
            }
        },
        {
            "src": "/graphql",
            "dest": "server/server.js"
        }
    ]
}